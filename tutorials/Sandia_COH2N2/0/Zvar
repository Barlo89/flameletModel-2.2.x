FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      csiv2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletfuel           
    {
	type 		fixedValue;
        value           uniform 0;
    }

    inletair           
    {
	type 		fixedValue;
        value           uniform 0;
    }

    outlet          
    {
        type            zeroGradient;
    }

    sidewall       
    {
        type            zeroGradient;
    }

    burnerwall       
    {
        type            zeroGradient;
    }

    front    
    {
        type            wedge;
    }

    back    
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }

